{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#030213",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#ffe100",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#f4f4f5",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#ececf0",
					"name": "Muted",
					"slug": "muted"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Space Grotesk\", sans-serif",
					"name": "Space Grotesk",
					"slug": "heading"
				},
				{
					"fontFamily": "\"Space Mono\", monospace",
					"name": "Space Mono",
					"slug": "body"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--background)"
				}
			},
			"core/group": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"style": "solid",
					"width": "0px"
				}
			},
			"core/navigation": {
				"border": {
					"width": "0px"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			}
		},
		"border": {
			"radius": "0px"
		},
		"elements": {
			"button": {
				"border": {
					"color": "var(--wp--preset--color--black)",
					"style": "solid",
					"width": "4px"
				},
				"spacing": {
					"padding": {
						"bottom": "12px",
						"left": "24px",
						"right": "24px",
						"top": "12px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "clamp(3rem, 6vw, 6rem)",
					"fontWeight": "700",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "clamp(2.25rem, 4.5vw, 4.5rem)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "clamp(1.875rem, 3vw, 3rem)",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "clamp(1.5rem, 2vw, 2.25rem)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "18px",
			"lineHeight": "1.6"
		}
	},
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}